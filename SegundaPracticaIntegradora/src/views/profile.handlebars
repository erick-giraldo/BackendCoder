{{#if message}}
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.18/dist/sweetalert2.min.js"></script>
  <script>
  Swal.fire({
    icon: 'error',
    title: 'No tiene Permiso para acceder a esta pÃ¡gina',
    text: '{{message}}',
    showConfirmButton: false
  }); setTimeout(() => {
        window.location.replace("/login");
      }, 1000); 
  </script>
{{else}}
<div class="content">
  <div class="container">
    <!-- end row -->
    <div class="row profile">
      <!-- end col -->
      <div class="col-lg-4">
        <div class="text-center card-box">
          <div class="member-card pt-2 pb-2">
            <div class="avatar">
                <div class="thumb-lg member-thumb mx-auto"><img
                src={{user.avatar}}
                class="rounded-circle img-thumbnail"
                alt="profile-image"
              /></div>
            <div class="power-off"><a
                id="btn-logout"
                class="p-off"
                data-placement="top"
                data-toggle="tooltip"
                class="tooltips"
                href="#"
                onclick="logout()"
                data-original-title="Facebook"
              ><i class="fa fa-power-off"></i></a></div>
            </div>
            <div class="">
              <h4> {{user.first_name}} {{user.last_name}} </h4>
              <p class="text-muted">@{{user.occupation}} 
                <span><a
                    href="#"
                    class="text-pink"
                  >{{user.email}}</a></span></p>
            </div>
            <ul class="social-links list-inline">
              <li class="list-inline-item"><a
                  title=""
                  class="facebook"
                  data-placement="top"
                  data-toggle="tooltip"
                  class="tooltips"
                  href=""
                  data-original-title="Facebook"
                ><i class="fa fa-facebook"></i></a></li>
              <li class="list-inline-item"><a
                  title=""
                  class="twitter"
                  data-placement="top"
                  data-toggle="tooltip"
                  class="tooltips"
                  href=""
                  data-original-title="Twitter"
                ><i class="fa fa-twitter"></i></a></li>
              <li class="list-inline-item"><a
                  title=""
                  class="instagram"
                  data-placement="top"
                  data-toggle="tooltip"
                  class="tooltips"
                  href=""
                  data-original-title="Instagram"
                ><i class="fa fa-instagram"></i></a></li>
            </ul>
            <button
              type="button"
              class="btn btn-primary mt-3 btn-rounded waves-effect w-md waves-light"
            >Message Now</button>
            <div class="mt-4">
              <div class="row">
                <div class="col-4">
                  <div class="mt-3">
                    <h4>4560</h4>
                    <p class="mb-0 text-muted">Wallets Balance</p>
                  </div>
                </div>
                <div class="col-4">
                  <div class="mt-3">
                    <h4>12350</h4>
                    <p class="mb-0 text-muted">Income amounts</p>
                  </div>
                </div>
                <div class="col-4">
                  <div class="mt-3">
                    <h4>742</h4>
                    <p class="mb-0 text-muted">Total Transactions</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- end col -->
    </div>

    <!-- end row -->
  </div>
  <!-- container -->
</div>
{{/if}}
<script type="module" src="/js/logout.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.18/dist/sweetalert2.min.js"></script>
