<div class="container-fluid padre">
  <div class="row hijo">
    <div class="col-lg-10 col-md-12 col-sm-12 main-container">
      <div class="row nosotros-contenido">
        <h1>Carrtito de Compras</h1>
        <div class="productos-container">
          <table class="table table-sm table-products">
            <thead>
              <tr>
                <th scope="colIdth>
                <th scope="col">id</th>
                <th scope="col">CÃ³digo</th>
                <th scope="col">Categoria</th>
                <th scope="col">Nombre</th>
                <th scope="col">Imagen</th>
                <th scope="col">Cantidad</th>
                <th scope="col">Precio Unitario</th>
                <th scope="col">Precio</th>
              </tr>
            </thead>
            <tbody>
              {{#each products}}
              {{this.page}}
              <tr>
                <td>{{this.id}}</td>
                <td>{{this.code}}</td>
                <td>{{this.category}}</td>
                <td>{{this.name}}</td>
                <td><img style="width: 15rem;" src={{this.image}} /></td>
                <td>{{this.quantity}}</td>
                <td class="price">$ {{this.price}}</td>
                <td class="price">$ {{this.totalPrice}}</td>

              </tr>
                {{/each}}
               <tr>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td><b>Total a pagar:</b></td>
                    <td><b>$ {{total}}</b></td>
                </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="/js/products.js"></script>
